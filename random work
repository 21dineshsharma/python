'''
def checkfer(a,b,c,n):
    for a in range(1,10):
        for b in range(1,10):
            for c in range(1,10):
                for n in range(3,50):
                    if a**n + b**n == c**n:
                        print (a,b,c,n,"first")
    else:
        print(a,b,c,n,"second")
x = int(input("write a"))
y = int(input("write b"))
z = int(input("write c"))
w = int(input("write n"))
checkfer(x,y,z,w)
