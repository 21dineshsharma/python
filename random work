'''
def is_triangle(a,b,c):
    if a>b+c or b>a+c or c>a+b:
        print("not possible")
    else:
        print("possible")

x = int(input(" first side"))
y = int(input("sescond side"))
z= int(input("third side"))
is_triangle(x,y,z)
